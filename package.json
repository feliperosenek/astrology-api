  "version": "1.0.0",
  "description": "Api para el calculos astrologicos",
  "main": "index.js",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --watch src src/index.js",
